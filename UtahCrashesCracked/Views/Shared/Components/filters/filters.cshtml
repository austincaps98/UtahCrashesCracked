@{
}

@model IEnumerable<string>

<div class="d-grid gap-2">

    @foreach (string t in Model)
    {
        <a asp-controller="Home" asp-action="Crashes" asp-route-filter="@t" asp-route-pageNum="1"
           class="btn btn-block @(ViewBag.SelectedType == t ? "btn-primary" : "btn-outline-secondary")" style="white-space:pre-line">@t</a>
    }
</div>
